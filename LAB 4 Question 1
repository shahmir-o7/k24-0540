#include <stdio.h>
main () {

int score;

printf("Enter your score\n");
scanf("%d", &score);

if (score >=90 && score <=100){
	printf("Grade A");}
else if (score <= 89 && score >= 80){
	printf("Grade B");}
else if (score <= 79 && score >= 70){
	printf("Grade C");}
else if (score <= 69 && score >= 60){
	printf("Grade D");}
else if (score < 60 ){
	printf("Grade F");}
else{
	printf("Invalid Score!");}

}
